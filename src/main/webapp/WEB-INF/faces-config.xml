<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">
	<application>
		<!-- Defines the resource bundle that contains the standard JSF messages 
			(overriding the ones provided). -->
		<message-bundle>br.ufes.informatica.webhealth.faces</message-bundle>
		<resource-bundle>
			<base-name>br.ufes.informatica.webhealth.core.view.messages</base-name>
			<var>msgsCore</var>
		</resource-bundle>
		<locale-config>
			<default-locale>pt-BR</default-locale>
		</locale-config>
	</application>
</faces-config>
